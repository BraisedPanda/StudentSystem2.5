<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.braisedpanda.shirotest.mapper.RolePermissionMapper">




    <!--根据roleId查找所有的permission-->
    <select id="getPermission" resultType="String">
        select permission from rolePermission where roleId=#{roleId}
    </select>


</mapper>